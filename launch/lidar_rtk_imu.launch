<launch>

  <arg name="scanning_frequency" default="360.0" />
  <node pkg="asr_sick_lms_400" type="sick_lms400" name="laserMapping" output="screen">
    <param name="enable_eRIS" value="0" /> <!-- options: 0,1 -->
    <param name="encoder_type" value="2" /> <!-- options: 0,1,2,3,4 #2phase detect,#4constant vel--> 
    <param name="scanning_frequency" value="$(arg scanning_frequency)" />
  </node>

  <node pkg="rtk_emlid_reach" type="rtk_parse_node.py" name="rtk_parse_node" output="screen">
    <param name="host" value="192.168.42.1" />
  </node>

  <node pkg="serial_rosnode" type="arduinoImu.py" output="screen">
    <param name="port" value="/dev/ttyUSB0" />
    <param name="baudRate" value="38400" />
  </node>
  
</launch>
